{
	"name": "@smartface/i18n",
	"version": "5.0.0-beta.0",
	"description": "Powerful internationalization module for Smartface based on formatjs",
	"main": "lib/index.js",
	"author": "Alim Öncül",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"scripts": {
